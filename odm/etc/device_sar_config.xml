<?xml version="1.0" encoding="UTF-8"?>

<SarDsiConfig>
    <!-- use for sar uncouple framework -->
    <sarVersion>2</sarVersion>
    <!-- only use for CN -->
    <deviceSarConfig device_name="flame">
        <prop prop_name="ro.boot.hwc">CN</prop>
        <!-- receiver,hotSpot(wifi_spot,(bluetooth,bluetooth_pan)),sarsensor1,sarsensor2,mcc,wifi,cable,hall,usb,lab -->
        <listeners>100000000001</listeners>
        <!-- receiver,lab -->
        <dsimap value="10">1</dsimap>
        <dsimap value="00">2</dsimap>
        <dsimap value="11">3</dsimap>
        <dsimap value="01">4</dsimap>
    </deviceSarConfig>

    <!-- only use for India -->
    <deviceSarConfig device_name="flame">
        <prop prop_name="ro.boot.hwc">India</prop>
        <!-- receiver,hotSpot(wifi_spot,(bluetooth,bluetooth_pan)),sarsensor1,sarsensor2,mcc,wifi,cable,hall,usb,lab -->
        <listeners>100000000000</listeners>
        <!-- receiver -->
        <dsimap value="1">1</dsimap>
        <dsimap value="0">2</dsimap>
    </deviceSarConfig>

    <!-- default configuration. -->
    <deviceSarConfig device_name="flame">
        <prop prop_name="ro.boot.hwc">default</prop>
        <prop prop_name="ro.product.mod_device">default</prop>
        <listeners>100000000001</listeners>
        <!-- receiver,lab -->
        <dsimap value="10">1</dsimap>
        <dsimap value="00">2</dsimap>
        <dsimap value="11">3</dsimap>
        <dsimap value="01">4</dsimap>
    </deviceSarConfig>

</SarDsiConfig>
